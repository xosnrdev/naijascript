# Mathematical built-ins - NaijaScript
# Tests custom mathematical functions vs language equivalents

make sum get 0
make i get 1

jasi (i small pass 10001) start
    make sqrt_val get sqrt(i)
    make abs_val get abs(sqrt_val minus 50)
    make floor_val get floor(abs_val)
    make ceil_val get ceil(abs_val)
    sum get sum add floor_val add ceil_val
    i get i add 1
end

shout(sum)