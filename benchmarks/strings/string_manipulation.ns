# String manipulation - NaijaScript
# Tests built-in string functions

make text get "The Quick Brown Fox Jumps Over The Lazy Dog"
make count get 0
make i get 0

jasi (i small pass 10000) start
    make upper_text get upper(text)
    make lower_text get lower(text)
    make pos get find(lower_text, "fox")
    make sliced get slice(upper_text, 0, 10)
    count get count add len(sliced) add pos
    i get i add 1
end

shout(count)