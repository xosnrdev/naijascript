make x get 1
jasi (x small pass 16) start
    make fizz get 0
    if to say (x divide 3 na 0) start
        fizz get fizz add 1
    end
    if to say (x divide 5 na 0) start
        fizz get fizz add 2
    end
    if to say (fizz na 0) start
        shout (x)
    end
    if to say (fizz na 1) start
        shout (111)
    end
    if to say (fizz na 2) start
        shout (222)
    end
    if to say (fizz na 3) start
        shout (333)
    end
    x get x add 1
end
